
@{
    ViewBag.Title = "فاکتور";
}


<div id="showOrder">
    @Html.Action("Order")
</div>





<script>
    function commandOrder(id, count) {
        $.ajax({
            url: "/ShopCart/CommandOrder/" + id,
            type: "GET",
            data: { count: count }
        }).done(function (res) {
            $("#showOrder").html(res);
        });
    }
</script>
